{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "lastdino/procurement-flow",
    "description": "ProcurementFlow base package for procurement workflows.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lastdino\\ProcurementFlow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lastdino\\ProcurementFlow\\ProcurementFlowServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^12.0",
        "lastdino/approval-flow": "^0.1",
        "tuncaybahadir/quar": "^1.7",
        "lastdino/chrome-laravel": "^0.1",
        "spatie/laravel-medialibrary": "^11.0",
        "phpoffice/phpspreadsheet": "^5.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
